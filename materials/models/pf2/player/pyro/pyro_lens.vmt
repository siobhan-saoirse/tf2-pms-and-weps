"VertexLitGeneric"
{
	"$basetexture" "models/pf2/player/pyro/pyro_red"
	"$bumpmap" "models/pf2/player/pyro/pyro_normal"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" 0.0
	"$detailblendmode" 6

	"$phong" "1"
	"$phongexponenttexture" "models\pf2\player\pyro\pyro_exponent"
	"$phongboost" ".5"	
	"$lightwarptexture" "models/pf2/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[1 10 20]"
	"$halflambert" "1"

	// Rim lighting parameters
//	"$rimlight" "1"												// To enable rim lighting (requires phong)
//	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
//	"$rimlightboost" "2"										// Boost for ambient cube component of rim lighting

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
	}
}

